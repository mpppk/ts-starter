{
  "name": "ts-starter",
  "version": "0.0.1",
  "scripts": {
    "start": "NODE_ENV=production tsx src/index.ts",
    "start:dev": "op run --env-file='.env' -- tsx src/index.ts",
    "format": "prettier --write .",
    "watch:type-check": "npx tsc --noEmit --watch",
    "test": "run-p test:*",
    "test:unit": "vitest",
    "test:lint": "eslint .",
    "test:format": "prettier --check .",
    "test:type-check": "tsc --noEmit"
  },
  "author": "mpppk",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all2": "^5.0.0",
    "prettier": "^2.8.7",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vitest": "^0.34.0",
    "zod": "^3.21.4"
  }
}
